            "sqlname": "amLogicalDrive",
            "label": "Logical drives",
            "filter": "Type = 'NTFS'",
            "orderby": "dtLastModif",
            "fields": [
              {
                "sqlname": "dtLastModif",
                "label": "Modified on",
                "type": "Date+Time",
                "size": "8",
                "alias": "Modified_On"
              },
              {
                "sqlname": "Device",
                "label": "Path",
                "type": "String",
                "size": "256",
                "alias": "Driver path"
              }
            ]
          }
        },
        {
          "reversefield": "Portfolio.lPortfolioItemId",
          "sqlname": "Softs",
          "label": "Software installation",
          "reverse": "ParentPortfolio",
          "body": {
            "sqlname": "amSoftInstall",
            "label": "Software installations or utilizations",
            "filter": "seType = 0",
            "orderby": "dtLastUse",
            "fields": [
              {
                "sqlname": "dtLastModif",
                "label": "Modified on",
                "type": "Date+Time",
                "size": "8",
                "alias": "Modified_On"
              },
              {
                "sqlname": "Model.Name",
                "label": "Name",
                "type": "String",
                "size": "129",
                "alias": "Soft Name"
              }
            ]
          }
        }
      ]
    },
