          "label": "Asset tag",
          "type": "String",
          "size": "81",
          "alias": "My Asset Tag"
        },
        {
          "sqlname": "Portfolio.Asset.SerialNo",
          "label": "Serial #",
          "type": "String",
          "size": "129",
          "alias": "SN."
        },
        {
          "sqlname": "Portfolio.lPortfolioItemId",
          "label": "Portfolio item ID",
          "type": "Long",
          "size": "4",
          "PK": true
        },
        {
          "sqlname": "lComputerId",
          "label": "IT equipment ID",
          "type": "Long",
          "size": "4",
          "PK": true
        }
      ],
      "links": [
        {
          "reversefield": "lComputerId",
          "sqlname": "LogicalDrives",
          "label": "Logical drives",
          "reverse": "Computer",
